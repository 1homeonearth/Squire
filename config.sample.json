{
  "token": "$ENV{DISCORD_TOKEN}",
  "applicationId": "$ENV{APPLICATION_ID}",
  "devGuildId": null,
  "dbPath": "./squire.db.json",
  "sampleRate": 1.0,
  "forwardBots": true,
  "debugLevel": "info",
  "loggingServerId": "$ENV{LOGGING_SERVER_ID}",
  "featureOrder": [
    "setup",
    "auto-bouncer",
    "moderation-commands",
    "logging-forwarder",
    "rainbow-bridge",
    "welcome-cards",
    "experience",
    "playlists"
  ],
  "mapping": "$ENV{MAPPING_JSON}",
  "loggingChannels": "$ENV{LOGGING_CHANNELS_JSON}",
  "loggingWebhookMeta": {},
  "excludeChannels": "$ENV{EXCLUDE_CHANNELS_JSON}",
  "excludeCategories": "$ENV{EXCLUDE_CATEGORIES_JSON}",
  "rainbowBridge": {
    "forwardBots": true,
    "bridges": "$ENV{RAINBOW_BRIDGE_BRIDGES_JSON}"
  },
  "autoban": {
    "enabled": true,
    "blockedUsernames": ["mega", "megas", "link", "links"],
    "notifyChannelId": "$ENV{AUTOBAN_NOTIFY_CHANNEL_ID}",
    "notifyWebhookUrls": [
      "$ENV{AUTOBAN_NOTIFY_WEBHOOK}"
    ],
    "testRoleMap": "$ENV{AUTOBAN_TEST_ROLE_MAP_JSON}",
    "deleteMessageSeconds": 0,
    "scanBio": true
  },
  "embedBuilder": {},
  "experience": "$ENV{EXPERIENCE_CONFIG_JSON}",
  "mainServerIds": "$ENV{MAIN_SERVER_IDS_JSON}",
  "welcome": "$ENV{WELCOME_CONFIG_JSON}",
  "playlists": {
    "spotify": {
      "clientId": "$ENV{SPOTIFY_CLIENT_ID}",
      "clientSecret": "$ENV{SPOTIFY_CLIENT_SECRET}",
      "refreshToken": "$ENV{SPOTIFY_REFRESH_TOKEN}",
      "playlistId": "$ENV{SPOTIFY_PLAYLIST_ID}",
      "skipDupes": "$ENV{PLAYLISTS_SKIP_DUPES}"
    },
    "youtube": {
      "clientId": "$ENV{YT_CLIENT_ID}",
      "clientSecret": "$ENV{YT_CLIENT_SECRET}",
      "refreshToken": "$ENV{YT_REFRESH_TOKEN}",
      "playlistId": "$ENV{YT_PLAYLIST_ID}"
    }
  },
  "moderationCommands": {
    "roleMap": "$ENV{MODERATION_ROLE_MAP_JSON}"
  }
}

