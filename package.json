{
  "name": "squire",
  "version": "2.0.0",
  "license": "GPL-3.0-only",
  "description": "Discord bot for The Unbreakable Crown",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "vitest run",
    "build": "tsc -p . || true",
    "start": "node index.js",
    "deploy:commands": "node src/core/commands-deploy.js",
    "cmds:list": "node src/core/commands-list.js",
    "cmds:wipe:global": "node src/core/commands-wipe.js --global",
    "cmds:wipe:guild": "node src/core/commands-wipe.js --guild"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "canvacord": "^6.0.4",
    "discord.js": "^14.24.0",
    "dotenv": "^16.4.5",
    "lokijs": "^1.5.12"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "eslint": "^9.12.0",
    "globals": "^15.9.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.5"
  }
}
